{
  "manifest_version": 2,
  "name": "Lorem Ipsum Shortcut",
  "description": "Creates a shortcut to filling out Lorem Ipsum",
  "version": "1.0",
  "icons": {
    "48": "icons/icon.png",
    "96": "icons/icon@2x.png"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],
  "browser_action": {
    "default_title": "Lorem Ipsum Shortcut",
    "default_popup": "popup.html"
  }
}